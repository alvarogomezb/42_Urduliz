/*   Created: 2022/08/29 11:44:12 by agomez-b          #+#    #+#             */
/*   Updated: 2022/09/23 13:19:01 by agomez-b         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

int	ft_strncmp(const char *s1, const char *s2, size_t n)
{
	unsigned int	i;

	i = 0;
	if (n == 0)
	{
		return (0);
	}
	while (*s1 && *s2 && *s1 == *s2 && (i < (n - 1)))
	{
		s1 ++;
		s2 ++;
		i ++;
	}
	return (*s1 - *s2);
}
