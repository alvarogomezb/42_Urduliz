/*   Updated: 2022/09/17 18:49:20 by agomez-b         ###   ########.fr       */
/*                                                                            */
/* ************************************************************************** */

#include "libft.h"

char	*ft_strchr(const char *s, int c)
{
	while (*s != '\0' && *s != c)
		s ++;
	if (*s == c)
		return (s);
	else
		return (NULL);
}
